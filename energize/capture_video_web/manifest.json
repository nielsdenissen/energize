{
  "name": "Energizer",
  "version": "1.0",
  "description": "Show energy of meeting using ML and Google Meet",
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": [
    "declarativeContent",
    "activeTab",
    "tabCapture",
    "tabs",
    "<all_urls>",
    "*://*/*"
  ],
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
